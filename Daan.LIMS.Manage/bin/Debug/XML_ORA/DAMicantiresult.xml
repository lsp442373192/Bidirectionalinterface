  <statements>

    <select id="SelectDAMicantiresult" parameterClass="string" resultMap="DAMicantiresultResult">
      SELECT MICANTIRESULTID AS Micantiresultid,HOSPSAMPLENUMBER AS Hospsamplenumber,HOSPSAMPLEID AS Hospsampleid,REQUESTCODE AS Requestcode,MICORGRESULTID AS Micorgresultid,ANTICODE AS Anticode,ANTINAME AS Antiname,ENGANTINAME AS Engantiname,DISPLAYORDER AS Displayorder,RESULTVALUE AS Resultvalue,TESTRESULT AS Testresult,SRANGE AS Srange,RRANGE AS Rrange,CREATEDATE AS Createdate
      FROM DA_MICANTIRESULT
      <dynamic prepend="WHERE">
        <isParameterPresent>
          MICANTIRESULTID = #value#
        </isParameterPresent>
      </dynamic>
    </select>
    <insert id="InsertDAMicantiresult" parameterClass="DAMicantiresult">
      INSERT INTO DA_MICANTIRESULT
      (MICANTIRESULTID,HOSPSAMPLENUMBER,HOSPSAMPLEID,REQUESTCODE,MICORGRESULTID,ANTICODE,ANTINAME,ENGANTINAME,DISPLAYORDER,RESULTVALUE,TESTRESULT,SRANGE,RRANGE,CREATEDATE)
      VALUES(SEQ_DA_MICANTIRESULT.Nextval,#Hospsamplenumber#,#Hospsampleid#,#Requestcode#,#Micorgresultid#,#Anticode#,#Antiname#,#Engantiname#,#Displayorder#,#Resultvalue#,#Testresult#,#Srange#,#Rrange#,sysdate)
    </insert>
    <update id="UpdateDAMicantiresult" parameterClass="DAMicantiresult">
      UPDATE DA_MICANTIRESULT
      SET MICANTIRESULTID=#Micantiresultid#,HOSPSAMPLENUMBER=#Hospsamplenumber#,HOSPSAMPLEID=#Hospsampleid#,REQUESTCODE=#Requestcode#,MICORGRESULTID=#Micorgresultid#,ANTICODE=#Anticode#,ANTINAME=#Antiname#,ENGANTINAME=#Engantiname#,DISPLAYORDER=#Displayorder#,RESULTVALUE=#Resultvalue#,TESTRESULT=#Testresult#,SRANGE=#Srange#,RRANGE=#Rrange#,CREATEDATE=#Createdate#
      WHERE MICANTIRESULTID = #Micantiresultid#
    </update>
    <delete id="DeleteDAMicantiresultByRequestCode" parameterClass="string">
      DELETE FROM DA_MICANTIRESULT
      WHERE REQUESTCODE=#Requestcode#
    </delete>
  </statements>
