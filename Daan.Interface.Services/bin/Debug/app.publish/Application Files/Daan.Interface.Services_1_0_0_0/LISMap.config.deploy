    <sqlMap embedded="XML${type}.DATablelastdate.xml,Daan.Interface.Mapconfig"/>
    <sqlMap embedded="XML${type}.DAReport.xml,Daan.Interface.Mapconfig"/>
    <sqlMap embedded="XML${type}.DAOutspecimen.xml,Daan.Interface.Mapconfig"/>
    <sqlMap embedded="XML${type}.DAErrorlog.xml,Daan.Interface.Mapconfig"/>
    <sqlMap embedded="XML${type}.DAPathologyresult.xml,Daan.Interface.Mapconfig"/>
    <sqlMap embedded="XML${type}.DALisPicture.xml,Daan.Interface.Mapconfig"/>
    <sqlMap embedded="XML${type}.DAResult.xml,Daan.Interface.Mapconfig"/>
    <sqlMap embedded="XML${type}.DAMicorgresult.xml,Daan.Interface.Mapconfig"/>
    <sqlMap embedded="XML${type}.DAMicantiresult.xml,Daan.Interface.Mapconfig"/>
    <sqlMap embedded="XML${type}.DASoftversion.xml,Daan.Interface.Mapconfig"/>
    <sqlMap embedded="XML${type}.DADictuser.xml,Daan.Interface.Mapconfig"/>
    <sqlMap embedded="XML${type}.DAOperationlog.xml,Daan.Interface.Mapconfig"/>
	  <sqlMap embedded="XML${type}.DAOutspecimentest.xml,Daan.Interface.Mapconfig"/>
    <sqlMap embedded="XML${type}.DATestmap.xml,Daan.Interface.Mapconfig"/>
    <sqlMap embedded="XML${type}.VDaLisrequest.xml,Daan.Interface.Mapconfig"/>
    <sqlMap embedded="XML${type}.VDAListests.xml,Daan.Interface.Mapconfig"/>
    <sqlMap embedded="XML${type}.VDaLisrequesttest.xml,Daan.Interface.Mapconfig"/>
